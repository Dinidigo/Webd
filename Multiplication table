<!DOCTYPE html>
<html>
    <head>
        <title>Multiplication table</title>
    </head>
    <body>
        <script type="text/javascript">
            var n=prompt("Enter positivr value of n: ");
            if(!isNaN(n)){
                var number="";
                var table="";
                for(var i=1; i<=10; i++){
                    number=n*i;
                    table+=n+"*"+i+"="+number+"\n";
                }
                alert(table);

            }
            else{
                alert("enter a positive");
                n=prompt("Enter a positive value of n: ");
            }
            document.write(n+"table values displayed using alert..")
            
        </script>
    </body>
</html>
